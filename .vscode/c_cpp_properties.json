    "configurations": [
        {
            "name": "miosix",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            //"defines": ["_BOARD_STM32F4DISCOVERY"],
            "compilerPath": "/opt/arm-miosix-eabi/bin/arm-miosix-eabi-g++",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [],
            "intelliSenseModeIsExplicit": true,
